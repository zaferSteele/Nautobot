# This section defines a group of hosts called "webservers"
[webservers]

# Below we define a single host entry:
#  - "n1" is the name we will use to refer to the nautobot instance/host in the playbook
#  - ansible_host specifies the actual IP address or DNS name of the machine.
#  - ansible_ssh_private_key_file points to the private SSH key to use when connecting.

[webservers]
n1 ansible_host=192.168.3.2 ansible_ssh_private_key_file=~/.ssh/id_rsa
